{
  "name": "function-toogle",
  "displayName": "function-toogle",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "function-toogle.helloWorld",
        "title": "Activate"
      }
    ]
  },
  "scripts": {
